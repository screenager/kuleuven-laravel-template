{
  "name": "kuleuven/laravel-template",
  "description": "Boilerplate for Laravel for setting op the KU Leuven -Bootstrap-powered- 2016 lay-out",
  "type": "library",
  "keywords": ["design", "layout", "kul", "ku leuven", "laravel"],
  "authors": [
    {
      "name": "Sven Peremans",
      "email": "sven.peremans@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.*",
    "davejamesmiller/laravel-breadcrumbs": "^3.0",
    "lavary/laravel-menu": "^1.6",
    "mcamara/laravel-localization": "^1.1",
    "stevenmaguire/laravel-middleware-csp": "^0.1.1",
    "laravelcollective/html": "^5.3",
    "mews/purifier": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Screenager\\KULeuvenBoilerplate\\": "src/"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}